{{#each items}}
    
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row>
        <c t="inlineStr"><is><t></t></is></c>
        <c t="inlineStr"><is><t>{{partId}}</t></is></c>
        <c t="inlineStr"><is><t>{{orderId}}</t></is></c>
       <c t="inlineStr"><is><t>{{meltNo}}</t></is></c>
       <c t="inlineStr"><is><t>{{batchId}}</t></is></c>
        <c t="inlineStr"><is><t>{{itemName}}</t></is></c>
        <c t="inlineStr"><is><t>{{itemType}}</t></is></c>
         <c t="inlineStr"><is><t>{{#batchDateTime meltDate}}{{/batchDateTime}}</t></is></c>
        <c t="inlineStr"><is><t>{{rawMaterialTotalWeight}}</t></is></c>
        <c t="inlineStr"><is><t>{{consumablesTotalWeight}}</t></is></c>
        <c t="inlineStr"><is><t>{{perKgCost}}</t></is></c>
        <c t="inlineStr"><is><t>{{totalCostRawMaterial}}</t></is></c>
        <c t="inlineStr"><is><t>{{totalCostConsumables}}</t></is></c>
    </row>
{{/xlsxAdd}}
{{/each}}
{{{xlsxPrint}}}